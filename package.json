{
  "name": "jira-issuer",
  "version": "1.0.0",
  "private": true,
  "description": "A command line tool to create bulk JIRA issues based on JSON template files.",
  "author": "Nikolas Vourlakis <nvourlakis@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git@github.com:webtrails/jira-issuer.git"
  },
  "bugs": "https://github.com/webtrails/jira-issuer/issues",
  "main": "app.js",
  "files": [
    "bin/",
    "config/",
    "lib/"
  ],
  "scripts": {
    "lint": "eslint src test",
    "check": "flow check",
    "test": "NODE_ENV=testing ava",
    "coverage": "NODE_ENV=testing nyc ava",
    "build": "npm run lint && rm -rf lib/* && babel src -d lib"
  },
  "ava": {
    "require": "babel-register",
    "babel": {
      "babelrc": true
    }
  },
  "engines": {
    "node": ">= 8"
  },
  "directories": {
    "bin": "bin",
    "lib": "lib",
    "test": "test"
  },
  "license": "MIT",
  "keywords": [
    "jira",
    "cli"
  ],
  "dependencies": {
    "chalk": "^2.0.1",
    "commander": "^2.11.0",
    "jira-client": "^6.4.0"
  },
  "devDependencies": {
    "ava": "^0.20.0",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-preset-flow": "^6.23.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-import": "^2.6.1",
    "eslint-plugin-prettier": "^2.1.2",
    "flow-bin": "^0.49.1",
    "nyc": "^11.0.3",
    "prettier": "^1.5.2"
  }
}
